<https://diyhistory.org/miike-db/api/items/21> a ns1:Item ;
    ns3:acquisitions "積氏" ;
    ns3:alternateFormsAvailable "デジタル画像" ;
    ns3:level "アイテ" ;
    ns1:created "2018-10-06T03:33:26+00:00"^^xsd:dateTime ;

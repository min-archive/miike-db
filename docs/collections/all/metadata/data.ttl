<https://diyhistory.org/miike-db/api/items/37> a ns1:Item ;
    ns3:acquisitions "江嶋氏" ;
    ns3:alternateFormsAvailable "デジタル画像" ;
    ns3:level "ファイル" ;
    ns1:created "2018-10-06T03:33:26+00:00"^^xsd:dateTime ;
